{
  "version": 2,
  "alias": ["fs-usage.now.sh"],
  "env": {
    "WP_URL": "@wp-url"
  },
  "builds": [
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/mail_templates/(?<id>[^/]+)$", "dest": "mail_templates/show?id=$id" },
    { "src": "/documents/(?<slug>[^/]+)$", "dest": "documents/index?slug=$slug" }
  ]
}